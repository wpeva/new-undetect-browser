{
  "name": "undetect-browser",
  "version": "1.0.0",
  "description": "Advanced anti-detection browser based on Chromium with comprehensive stealth capabilities",
  "main": "dist/electron/main.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:ci": "tsc --project tsconfig.build.json",
    "build:watch": "tsc --watch",
    "build:clean": "rm -rf dist && npm run build",
    "dev": "tsc --watch",
    "server": "npm run build && node dist/server/index.js",
    "server:v2": "npm run build && node dist/server/index-v2.js",
    "server:dev": "nodemon --watch server --exec \"npm run build && node dist/server/index.js\"",
    "server:dev:v2": "nodemon --watch server --exec \"npm run build && node dist/server/index-v2.js\"",
    "electron": "npm run build && electron .",
    "electron:dev": "npm run build && NODE_ENV=development electron .",
    "electron:build": "npm run build && electron-builder --win",
    "electron:build:all": "npm run build && electron-builder --win --mac --linux",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:detection": "jest --testPathPattern=detection",
    "test:unit": "jest --testPathPattern=unit",
    "test:unit:new": "jest tests/unit/memoization.test.ts tests/unit/performance.test.ts",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e --testTimeout=120000",
    "test:e2e:platform": "jest tests/e2e/full-platform.test.ts --testTimeout=120000",
    "test:e2e:detection": "jest tests/e2e/detection-comprehensive.test.ts --testTimeout=120000",
    "test:e2e:performance": "jest tests/e2e/performance-metrics.test.ts --testTimeout=120000",
    "test:load": "k6 run tests/load/stress-test.js",
    "test:load:quick": "k6 run tests/load/quick-load-test.js",
    "test:performance": "npm run test:e2e:performance",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.ts'",
    "test:coverage:unit": "jest --coverage --testPathPattern=unit --collectCoverageFrom='src/**/*.ts'",
    "test:ci": "jest --ci --maxWorkers=2 tests/unit/memoization.test.ts tests/unit/performance.test.ts",
    "test:ci:coverage": "jest --ci --coverage --maxWorkers=2 tests/unit/memoization.test.ts tests/unit/performance.test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run test:unit:new",
    "prepublishOnly": "npm run validate && npm run build:clean",
    "precommit": "npm run lint:fix && npm run format",
    "ci": "npm run validate && npm run test:ci"
  },
  "build": {
    "appId": "com.undetectbrowser.app",
    "productName": "UndetectBrowser",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/icon.png",
      "category": "Development"
    }
  },
  "keywords": [
    "browser",
    "automation",
    "puppeteer",
    "playwright",
    "stealth",
    "anti-detection",
    "fingerprint",
    "undetectable",
    "bot",
    "scraping",
    "testing",
    "antidetect",
    "multilogin"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^6.0.0",
    "chrome-launcher": "^1.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "electron-store": "^8.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "faker": "^5.5.3",
    "fingerprint-generator": "^2.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.0.2",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "playwright": "^1.48.0",
    "playwright-extra": "^4.3.6",
    "proxy-chain": "^2.5.4",
    "puppeteer": "^23.10.4",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "redis": "^5.9.0",
    "socket.io": "^4.8.1",
    "tough-cookie": "^4.1.3",
    "user-agents": "^1.1.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "optionalDependencies": {
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.7.5",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.2",
    "@types/node-fetch": "^2.6.11",
    "@types/pg": "^8.15.6",
    "@types/tough-cookie": "^4.0.5",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
