{
  "name": "undetect-browser",
  "version": "1.0.0",
  "description": "Advanced anti-detection browser based on Chromium with comprehensive stealth capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:clean": "rm -rf dist && npm run build",
    "dev": "tsc --watch",
    "server": "npm run build && node dist/server/index.js",
    "server:dev": "nodemon --watch server --exec \"npm run build && node dist/server/index.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:detection": "jest --testPathPattern=detection",
    "test:unit": "jest --testPathPattern=unit",
    "test:unit:new": "jest tests/unit/memoization.test.ts tests/unit/performance.test.ts",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.ts'",
    "test:coverage:unit": "jest --coverage --testPathPattern=unit --collectCoverageFrom='src/**/*.ts'",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run test:unit:new",
    "prepublishOnly": "npm run validate && npm run build:clean",
    "precommit": "npm run lint:fix && npm run format",
    "ci": "npm run validate && npm run test:ci"
  },
  "keywords": [
    "browser",
    "automation",
    "puppeteer",
    "playwright",
    "stealth",
    "anti-detection",
    "fingerprint",
    "undetectable",
    "bot",
    "scraping",
    "testing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "bcrypt": "^6.0.0",
    "chrome-launcher": "^1.1.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "faker": "^5.5.3",
    "fingerprint-generator": "^2.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "playwright": "^1.48.0",
    "playwright-extra": "^4.3.6",
    "proxy-chain": "^2.5.4",
    "puppeteer": "^23.10.4",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "socket.io": "^4.8.1",
    "tough-cookie": "^4.1.3",
    "user-agents": "^1.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.2",
    "@types/tough-cookie": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
