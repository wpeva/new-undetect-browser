{
  "name": "undetect-browser",
  "version": "1.0.0",
  "description": "Advanced anti-detection browser based on Chromium with comprehensive stealth capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:detection": "jest --testPathPattern=detection",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "browser",
    "automation",
    "puppeteer",
    "playwright",
    "stealth",
    "anti-detection",
    "fingerprint",
    "undetectable",
    "bot",
    "scraping",
    "testing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "puppeteer": "^21.6.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "playwright": "^1.40.1",
    "playwright-extra": "^4.3.6",
    "chrome-launcher": "^1.1.0",
    "proxy-chain": "^2.4.0",
    "tough-cookie": "^4.1.3",
    "user-agents": "^1.1.0",
    "fingerprint-generator": "^2.1.0",
    "faker": "^5.5.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.11",
    "@types/tough-cookie": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
